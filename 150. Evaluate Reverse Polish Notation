class Solution {
public:
    int evalRPN(vector<string>& tokens) {
        
        stack<int>st;
        int n = tokens.size();

        for(int i=0 ; i<n ; i++){

            string token = tokens[i];

            if((token == "+") || (token=="-" )|| (token =="*") || (token=="/")){

              int a = st.top();
              st.pop();

              int b = st.top();
              st.pop();
            
              int result;
            if(token=="+"){
                result = b+a;
            }
            else if(token=="-"){
                 result = b-a;
            }
           else if(token=="*"){
                 result = b*a;
            }

            else if(token=="/"){
                result = b/a;
            }

            st.push(result);
        }

        else st.push(stoi(token));
    }

        return st.top();
    }
};
